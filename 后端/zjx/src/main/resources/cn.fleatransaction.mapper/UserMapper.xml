<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.fleatransaction.mapper.UserMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="appResultMap" type="cn.fleatransaction.entity.User">
        <result column="user_id" property="userId"/>
        <result column="user_name" property="userName"/>
        <result column="user_email" property="userEmail"/>
        <result column="user_phone" property="userPhone"/>
        <result column="password" property="password"/>
        <result column="user_credit" property="userCredit"/>
        <result column="user_avatar" property="userAvatar"/>
        <result column="real_name" property="realName"/>
        <result column="id_card" property="idCard"/>
        <result column="stu_number" property="stuNumber"/>
        <result column="user_status" property="userStatus"/>
    </resultMap>
</mapper>
